/*remove delay tap*/
$.event.special.tap = {
    setup: function () {
        var self = this,
            $self = $(self);

        $self.on('touchstart', function (startEvent) {
            var target = startEvent.target;

            $self.one('touchend', function (endEvent) {
                if (target == endEvent.target) {
                    $.event.simulate('tap', self, endEvent);
                }
            });
        });
    }
};
